# ref: http://d.hatena.ne.jp/tokuhirom/20081212/1229080990
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color]
    ui = true
[core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[branch]
    autosetupmerge = true
[alias]
    st = status
    ci = commit -a
    br = branch
    co = checkout
    df = diff
    lg = log -p
